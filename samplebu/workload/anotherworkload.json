{
    "name": "anotherworkload",
    "service-location-port": ["http://loaker.example.com:8080"],
    "loading-style": null, 
    "value-base": 0,
    "value-top": 100000,
    "threshold": 800,
    "threshold-hit-continue": false,
    "task": [
        {
            "name": "basetask-authenticate",
            "threshold": 600,
            "iteration": 6,
            "continue-on-timeout": true,
            "maintain-connection": true
        },
        {
            "name": "sleep",
            "threshold": 5000,
            "iteration": 0
        },
        {
            "name": "basetask-logout",
            "threshold": 150,
            "iteration": 1,
            "continue-on-timeout": false,
            "maintain-connection": false
        }
    ]
}
