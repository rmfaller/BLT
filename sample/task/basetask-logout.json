{
    "name": "basetask-logout",
    "service-location-port": ["http://$BLT_AMHOST:$BLT_AMPORT"],
    "service-location-port": ["http://am0.example.com:8080", "http://am1.example.com:8080"],
    "loading-style": "roundrobin, random, ha", 
    "value-base": 0,
    "value-top": 100000,
    "request": "POST",
    "header": { 
        "X-OpenAM-Username": "user.$BLT_RANDOMVALUE",
        "X-OpenAM-Password": "password",
        "Content-Type": "application/json"
    },
    "url-endpoint": "/am/json",
    "url-payload": "user.$BLT_RANDOMVALUE",
    "data-payload": {
        "_id": "user.$BLT_RANDOMVALUE",
        "contactInformation": {
            "telephoneNumber": "888 111 $BLT_RANDOMVALUE",
            "emailAddress": "user.$BLT_RANDOMVALUE@example.com"
        },
        "name": {
            "familyName": "$BLT_RANDOMVALUE",
            "givenName": "User"
        },
        "displayName": "User $BLT_RANDOMVALUE",
        "manager": [{
                "_id": "user.666",
                "displayName": "User 666"
            }]
    }
}